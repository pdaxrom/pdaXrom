default normal
prompt 1
timeout 100
display pdax86.msg
F1 mx2help.txt
F2 trblst.txt
F3 mphelp.txt
F4 credits.txt
F5 trblst2.txt
label normal
  kernel kernel/bzImage
  append initrd=initrd.bin root=/dev/ram0 ramdisk_size=64000 video=vesa:ywrap,mtrr vga=788
label HD
  localboot 0x80
label Floppy
  localboot 0x00
